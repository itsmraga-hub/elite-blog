<section>
  <h1>Add new post</h1>
  <%= form_with model: @post_new, url: user_posts_path do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :text %>
    <%= form.text_area :text %>
  </div>
  <div>
    <%= form.submit "Add post" %>
  </div>
  <% end %>
</section>